#! /usr/bin/gnuplot 
set terminal jpeg enhanced size 1000,800
#set terminal wxt enhanced
############### Q ##############
set output "graphics/Q.jpeg"
set xlabel "Normalized radius, a_{N}" font "Helvetica,18"
set ylabel "Q_{m}" font "Helvetica,18"
set title "Q_{m}"
set xrange [0.9:1]
#set yrange [0.:0.6]
#set style line 5 lt 1 pt 1
set grid
plot "data/Q.dat" using 1:2 title "ReQ_{m}" with lines lw 2 , "data/Q.dat" using 1:3 title "ImQ_{m}" with lines lw 2 
replot
unset output

############## Q1
set output "graphics/Q1.jpeg"
set xlabel "Normalized radius, a_{N}" font "Helvetica,18"
set ylabel "Q_{1m}" font "Helvetica,18"
set title "Q_{1m}"
set xrange [0.9:1]
#set yrange [0.:0.6]
#set style line 5 lt 1 pt 1
set grid
plot "data/Q1.dat" using 1:2 title "ReQ_{1m}" with lines lw 2 , "data/Q1.dat" using 1:3 title "ImQ_{1m}" with lines lw 2 
replot
unset output

############## P
set output "graphics/P.jpeg" 
set xlabel "Normalized radius, a_{N}" font "Helvetica,18"
set ylabel "P, [Pa]" font "Helvetica,18"
set title "P, [Pa]"
set xrange [0.:1]
#set yrange [0.:0.6]
#set style line 5 lt 1 pt 1
set grid
plot "data/P.dat" using 1:2 title "P_{e}" with lines lw 2 , "data/P.dat" using 1:3 title "P_{i}" with lines lw 2 
replot
unset output

############## P
set output "graphics/DP.jpeg" 
set xlabel "Normalized radius, a_{N}" font "Helvetica,18"
set ylabel "dP/da_{N}, [Pa]" font "Helvetica,18"
set title "dP, [Pa]"
set xrange [0.:1]
#set yrange [0.:0.6]
#set style line 5 lt 1 pt 1
set grid
plot "data/DP.dat" using 1:2 title "dP_{e}" with lines lw 2 , "data/DP.dat" using 1:3 title "P_{i}" with lines lw 2 
replot
unset output

############## bootstrap
set output "graphics/bootstrap.jpeg" 
set xlabel "Normalized radius, a_{N}" font "Helvetica,18"
set ylabel "bootstrap, [statampere/cm^2]" font "Helvetica,18"
set title "dP, [Pa]"
set xrange [0.:1]
#set yrange [0.:0.6]
#set style line 5 lt 1 pt 1
set grid
plot "data/Bootstrap.dat" using 1:2 title "dP_{e}" with lines lw 2 #, "data/DP.dat" using 1:3 title "P_{i}" with lines lw 2 
replot
unset output

exit



